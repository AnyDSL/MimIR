fn write(out: [float], src: [float], i: int) -> void {
    out[i] = src[i];
}

fn main() -> int {
    let width   = 2048;
    let height  = 2048;
    let mut arr = array(4, width, height);
    let mut out = array(4, width, height);

    let mapped = map(arr, 1, 1, (0, 0, 0), (width, height, 1));

    iteration(width, height, |x : int, y : int| -> void {
        write(out, mapped, x);
    });

    return 0;
}

