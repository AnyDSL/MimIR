// RUN: rm -f %t.ll
// RUN: %thorin -p clos %s --output-ll %t.ll -o -

.plugin core;

.fun f(a: .I32): .I32 =
    .let b = %core.wrap.mul 0 (a, a);
    return b;

.con .extern main [mem : %mem.M, argc : .I32, argv : %mem.Ptr (%mem.Ptr (.I8, 0), 0), return : .Cn [%mem.M, .I32]] =
    .ret r1 = f $ 42I32;
    .ret r2 = f $ r1;
    return (mem, r2);
