// RUN: %thorin %s -o - | FileCheck %s

.import mem;
.plugin regex;

.let i8 = .Idx 256;
.let bool = .Idx 2;

.con .extern disj(mem: %mem.M, string: %mem.Ptr (i8, 0), return: .Cn [%mem.M, bool, %mem.Ptr (i8, 0)]) = {
    .let pattern = %regex.conj (%regex.disj (%regex.cls.d, %regex.cls.D), %regex.disj (%regex.cls.w, %regex.cls.W), %regex.disj (%regex.cls.s, %regex.cls.S));
    return (%regex.match (mem, pattern, string))
    // CHECK: %regex.conj 3 ‹3; %regex.cls.any›
};
