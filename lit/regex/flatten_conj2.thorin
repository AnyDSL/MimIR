// RUN: %thorin %s -o - | FileCheck %s

.import mem;
.plugin regex;

.let i8 = .Idx 256;
.let bool = .Idx 2;

.con .extern flatten_conj(mem: %mem.M, string: %mem.Ptr (i8, 0), return: .Cn [%mem.M, bool, %mem.Ptr (i8, 0)]) = {
    .let pattern = %regex.conj (%regex.cls.d, %regex.conj (%regex.cls.d, %regex.cls.d), %regex.cls.d);
    return (%regex.match (mem, pattern, string))
    // CHECK: %regex.conj 4 ‹4; %regex.cls.d›
};
