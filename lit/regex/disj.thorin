// RUN: %thorin %s -o - | FileCheck %s

.import mem;
.plugin regex;

.let i8 = .Idx 256;
.let bool = .Idx 2;

.con .extern disj(mem: %mem.M, string: %mem.Ptr (i8, 0), return: .Cn [%mem.M, bool, %mem.Ptr (i8, 0)]) = {
    .let pattern = %regex.disj (%regex.cls.d, %regex.cls.w);
    return (%regex.match (mem, pattern, string))
    // CHECK: %regex.cls.w
};
