// RUN: %mim %s -o - -P %S | FileCheck %s

import mem;
plugin regex;

import dont_compile.mim;

let Top = ⊤:Nat;

con extern conj_a(mem: %mem.M 0, string: %mem.Ptr (<<Top; I8>>, 0), return: Cn [%mem.M 0, Bool, I32]) =
    let pattern = %regex.conj (%regex.lit 'a', %regex.empty);
    return (pattern (mem, string, 0I32));
// CHECK: %regex.range ‹2; 97I8›
// CHECK-NOT: %regex.conj
