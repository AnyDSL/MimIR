// RUN: %mim %s -o - -P %S | FileCheck %s

import mem;
plugin regex;

import dont_compile.mim;

let Top = ⊤:Nat;

con extern fold(mem: %mem.M 0, string: %mem.Ptr (<<Top; I8>>, 0), return: Cn [%mem.M 0, Bool, I32]) =
    let pattern = %regex.lit '0';
    return (pattern (mem, string, 0I32));
