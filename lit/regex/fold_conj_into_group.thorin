// RUN: %thorin %s -o - | FileCheck %s

.import mem;
.plugin regex;

.let i8 = .Idx 256;
.let bool = .Idx 2;

.con .extern fold_conj_into_group(mem: %mem.M, string: %mem.Ptr (i8, 0), return: .Cn [%mem.M, bool, %mem.Ptr (i8, 0)]) = {
    .let pattern = %regex.group (%regex.cls.w, %regex.conj (%regex.cls.d, %regex.cls.w), %regex.cls.d);
    return (%regex.match (mem, pattern, string))
    // CHECK: %regex.group 4 (%regex.cls.w, %regex.cls.d, %regex.cls.w, %regex.cls.d)
};
