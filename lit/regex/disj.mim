// RUN: %mim %s -o - -P %S | FileCheck %s

import mem;
plugin regex;

import dont_compile.mim;

let Top = ⊤:Nat;

con extern disj(mem: %mem.M, string: %mem.Ptr (<<Top; I8>>, 0), return: Cn [%mem.M, Bool, I32]) =
    let pattern = %regex.disj (%regex.cls.d, %regex.cls.w);
    return (pattern (mem, string, 0I32));
// CHECK: %regex.disj 2 (%regex.disj 2 (%regex.disj 2 (%regex.range (48:I8, 57:I8), %regex.range (65:I8, 90:I8)), %regex.range ‹2; 95:I8›), %regex.range (97:I8, 122:I8))
