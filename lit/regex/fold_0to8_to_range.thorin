.import mem;
.plugin regex;

.import dont_compile.thorin;

.let i8 = .Idx 256;
.let Top = ‚ä§:.Nat;

.con .extern fold(mem: %mem.M, string: %mem.Ptr (<<Top; .I8>>, 0), return: .Cn [%mem.M, .Bool, .I32]) = {
    .let pattern = %regex.disj 9 (%regex.lit 49:.I8, %regex.lit 48:.I8, %regex.lit 50:.I8, %regex.lit 51:.I8, %regex.lit 52:.I8, %regex.lit 53:.I8, %regex.lit 54:.I8, %regex.lit 55:.I8, %regex.lit 56:.I8);
