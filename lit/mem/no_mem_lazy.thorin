// RUN: rm -f %t.ll
// RUN: %thorin %s --output-ll %t.ll -o -

.plugin core;

.con f [a:.I32, ret: .Cn .I32] =
    .let b = %core.wrap.mul 0 (a, a);
    ret b;

.con .extern main [mem : %mem.M, argc : .I32, argv : %mem.Ptr (%mem.Ptr (.I8, 0), 0), return : .Cn [%mem.M, .I32]] =
    .con ret_cont r: .I32 =
        .con ret_cont2 r2: .I32 = return (mem, r2);
        f (r, ret_cont2);
    f (42:.I32,ret_cont);
