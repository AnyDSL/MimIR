// RUN: %mim %s

plugin math;
plugin refly;

lam constant (x: %math.F64): %math.F64 = 0.:%math.F64;
lam mul (x: %math.F64):%math.F64  = %math.arith.mul 0 (x, 0.:%math.F64);

let _ = %refly.equiv.struc_eq (constant,mul);

lam ident (x: %math.F32) : %math.F32 = x;
lam mulone (x:%math.F32) : %math.F32 = %math.arith.mul 0 (1.0:%math.F32, x);

let _ = %refly.equiv.struc_eq (ident, mulone);
