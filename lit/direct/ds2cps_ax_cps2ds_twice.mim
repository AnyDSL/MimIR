// RUN: rm -f %t.ll
// RUN: %mim %s --output-ll %t.ll -o - | FileCheck %s

plugin core;
plugin direct;

con f(a: I32, return: Cn I32)@tt = return (%core.wrap.add 0 (2I32, a));

con extern main(mem: %mem.M 0, argc: I32, argv: %mem.Ptr (%mem.Ptr (I8, 0), 0), return: Cn [%mem.M 0, I32]) =
    let g = %direct.cps2ds (I32,I32) f;
    let c1 = g 38I32;
    let h = %direct.cps2ds (I32,I32) f;
    let c2 = h (c1);
    return (mem, c2);

// CHECK-DAG: return{{.*}}42
