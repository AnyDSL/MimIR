// RUN: rm -f %t.ll
// RUN: %mim %s --output-ll %t.ll -o - | FileCheck %s

plugin core;
plugin direct;


lam f(a :I32): I32 = %core.wrap.add 0 (2I32, a);

fun h(mem: %mem.M 0, a: I32): [%mem.M 0, I32] =
    return (mem, f a);

fun extern main(mem: %mem.M 0, argc: I32, argv: %mem.Ptr0 (%mem.Ptr0 I8)): [%mem.M 0, I32] =
    h ((mem, 40I32), return);

// CHECK-DAG: return{{.*}}42
