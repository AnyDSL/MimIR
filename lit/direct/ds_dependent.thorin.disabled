// RUN: rm -f %t.ll
// RUN: %thorin %s --output-ll %t.ll -o - | FileCheck %s

.plugin core;
.plugin direct;


.lam U [n:.Nat] -> * = {
    .Idx n
};

.lam f [n:.Nat] -> (.Idx n) = {
    42:(.Idx n)
};

.con .extern main [mem : %mem.M, argc : .I32, argv : %mem.Ptr (%mem.Ptr (.I8, 0), 0), return : .Cn [%mem.M, .I32]] = {
    .let n = .i32;
    .let c = f n;
    return (mem, c)
};

// CHECK-DAG: return{{.*}}42
