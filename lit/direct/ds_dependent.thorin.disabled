// RUN: rm -f %t.ll ; \
// RUN: %thorin %s --output-ll %t.ll -o - | FileCheck %s

.plugin core;
.plugin direct;

.let I32 = %core.I32;

.lam U [n:.Nat] -> * = {
    .Idx n
};

.lam f [n:.Nat] -> (.Idx n) = {
    42:(.Idx n)
};

.con .extern main [mem : %mem.M, argc : I32, argv : %mem.Ptr (%mem.Ptr (%core.I8, 0), 0), return : .Cn [%mem.M, I32]] = {
    .let n = 4294967296;
    .let c = f n;
    return (mem, c)
};

// CHECK-DAG: return{{.*}}42
