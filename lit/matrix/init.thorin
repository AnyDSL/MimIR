// RUN: rm -f %t.ll ; \
// RUN: %thorin -d matrix -o - %s | FileCheck %s

.import core;
.import mem;
.import matrix;

.let _32 = 4294967296;
.let I32 = .Idx _32;

.con .extern f [mem : %mem.M, 
    [k:.Nat, l:.Nat],
    return: .Cn[%mem.M, %matrix.Mat (2, (k,l), I32)]] = {

    .let (mem2, M) = %matrix.init (2,(k,l),I32,mem);

    return (mem2, M)
};

// CHECK-NOT: %matrix.
