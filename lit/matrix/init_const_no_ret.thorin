// RUN: rm -f %t.ll ; \
// RUN: %thorin -o - %s | FileCheck %s

.plugin core;
.plugin matrix;

.con .extern f [mem : %mem.M, 
    [k:.Nat, l:.Nat],
    return: .Cn[%mem.M]] = {

    .let (mem2, M) = %matrix.constMat (2,(k,l),%core.I32) (mem, 0:%core.I32);

    return mem2
};

