// RUN: rm -f %t.ll ; \
// RUN: %thorin -o - %s | FileCheck %s

.plugin core;
.plugin matrix;

.let _32 = 4294967296;
.let I32 = .Idx _32;

.con .extern f [mem : %mem.M, 
    [k:.Nat, l:.Nat],
    return: .Cn[%mem.M]] = {

    .let (mem2, M) = %matrix.constMat (2,(k,l),I32) (mem, 0:I32);

    return mem2
};

// CHECK-NOT: %matrix.
