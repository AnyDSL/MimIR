// RUN: rm -f %t.ll
// RUN: %thorin %s --output-ll %t.ll -o -
.plugin core;

.lam PtrN(n: .Nat, T: *): * = (PtrN (%core.nat.sub (n, 1), %mem.Ptr0 T), T)#(%core.ncmp.e (n, 0));

.let _ = 23; // HACK

.fun .extern foo(mem: %mem.M, p: PtrN (4, .I32)): [%mem.M, PtrN (3, .I32)] = return (%mem.load (mem, p));
