// RUN: rm -f %t.ll
// RUN: %mim -p direct %s --output-ll %t.ll -o - | FileCheck %s

plugin core;
plugin autodiff;

con extern main [mem : %mem.M 0, argc : I32, argv : %mem.Ptr (%mem.Ptr (I8, 0), 0), return : Cn [%mem.M 0, I32]] =
    let t = %autodiff.add ((41I32, 42I32), (43I32, 44I32));
    let a = t#0_2;
    let b = t#1_2;
    let c = %core.wrap.mul 0 (a, b);
    return (mem,c);

