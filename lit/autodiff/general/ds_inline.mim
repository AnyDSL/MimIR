// RUN: rm -f %t.ll
// RUN: %mim %s --output-ll %t.ll -o - | FileCheck %s

plugin core;


lam f [a:I32]: I32 =
    %core.wrap.mul 0 (2I32, a);

con extern main [mem: %mem.M 0, argc: I32, argv: %mem.Ptr (%mem.Ptr (I8, 0), 0), return: Cn [%mem.M 0, I32]] =
    con ret_cont r: I32 = return (mem, r);
    let c = 42I32;
    let r = f c;
    ret_cont r;

// CHECK-DAG: return{{.*}}84
