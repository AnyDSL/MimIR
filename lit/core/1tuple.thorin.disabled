// RUN: rm -f %t.ll
// RUN: %thorin %s --output-ll %t.ll
// RUN: clang %t.ll -o %t -Wno-override-module
.plugin core;

.fun .extern foo(mem: %mem.M, p: %mem.Ptr0 «23; %core.I32») -> [%mem.M, %mem.Ptr0 %core.I32] =
    return (mem, %core.bitcast (%mem.Ptr0 %core.I32) p);

.fun .extern bar(mem: %mem.M, a b: %core.I32) -> [%mem.M, %core.I32] = return (mem, a);
