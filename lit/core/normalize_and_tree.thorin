.import core;

.lam .extern and_lit: .Cn [return : .Cn %Int 2] = {
    .ff,
    return
    (%core.bit2._and 2
        (%core.bit2._and 2
            (%core.bit2._and 2 (.tt, .tt),
             %core.bit2._and 2 (.ff, .ff)),
         %core.bit2._and 2
            (%core.bit2._and 2 (.ff, .tt),
             %core.bit2._and 2 (.tt, .ff))))
};

