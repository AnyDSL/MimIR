// RUN: rm -f %t.ll ; \
// RUN: %thorin %s --output-thorin - | FileCheck %s

.import core;

.cn .extern and_lit [return : .Cn %Int 2] = {
    return
    (%core.bit2._and 2
        (%core.bit2._and 2
            (%core.bit2._and 2 (.tt, .tt),
             %core.bit2._and 2 (.ff, .ff)),
         %core.bit2._and 2
            (%core.bit2._and 2 (.ff, .tt),
             %core.bit2._and 2 (.tt, .ff))))
};

