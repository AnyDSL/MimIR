// RUN: rm -f %t.ll ; \
// RUN: %thorin %s -o - | FileCheck %s

.import core;

.cn .extern and_lit [return : .Cn .Int 2] = {
    return
    (%core.bit2._and 2
        (%core.icmp.uge 2
            (.tt, .ff),
         %core.icmp.ug 2
            (.tt, .ff)))
};

// CHECK-DAG: and_lit _[[retId:[0-9_]+]]: .Cn (.Int 2)
// CHECK-DAG: _[[etaId:[0-9_]+]] 1:(.Int 2)

// CHECK-DAG: _[[etaId]] _[[etaVar:[0-9_]+]]: (.Int 2)
// CHECK-DAG: _[[retId]] _[[etaVar]]
