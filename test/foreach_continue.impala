/*
foreach with a continue
expected result: 15
*/
fn count(n : int, yield : fn(int, fn()), ret : fn()) {
    fn loop(i : int) {
        if (i < n)
            yield(i, || { loop(i+1); });
        else
            ret();
    }
    loop(0);
}

fn main() -> int {
    var b : int = 0;
    foreach count(10) |x : int| -> void {
        if (b > 10)
            continue;
        b += x;
    }
    return b;
}
