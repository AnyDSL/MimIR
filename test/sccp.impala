extern fn foo() -> int {
    var a : int;
    var b : int;
    var c : int;
    var d : int;
    var e : int;
    var f : int = 0;
    var g : int;

    a = 3;
    d = 2;

    while (a != d) {
        f = a + d;
        g = 5;
        a = g - d;

        if (f <= g) {
            f = g + 1;
            break;
        } else {
            if (g >= a)
                return f;
        }

        d = 2;
    }

    return f;
}
