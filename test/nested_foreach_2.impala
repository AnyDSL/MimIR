/*
three nested foreachs with break and continue
expected result: 190
*/
fn count(n : int, yield : fn(int, fn()), ret : fn()) {
    fn loop(i : int) {
        if (i < n)
            yield(i, || { loop(i+1); });
        else
            ret();
    }
    loop(0);
}

fn main() -> int {
    b : int = 0;
    foreach (x : int  <- count(10)) {
        b += x;
        foreach (y : int  <- count(x)) {
            b += y;
            if (y > 3)
                continue;
            foreach (z : int  <- count(y)) {
                b += z;
                if (z > 5)
                    break;
            }
        }
    }
    return b;
}
