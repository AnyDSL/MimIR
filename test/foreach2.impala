/*
foreach without float
*/
fn count(n : float, yield : fn(float, fn()), ret : fn()) {
    fn loop(i : float) {
        if (i < n)
            yield(i, || { loop(i+1.0f); });
        else
            ret();
    }
    loop(0.0f);
}

fn main() -> float {
    b : float = 2.0f;
    foreach (x : float  <- count(10.0f)) {
        b += x;
    }

    x : float = 42.0f;
    foreach (x  <- count(10.0f)) {
        b += x;
    }

    return b;
}
