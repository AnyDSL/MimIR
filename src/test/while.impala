def my_while<L, O>(l : L,
                head : pi(L, pi(sigma(L, bool)), 
                body : pi(L, pi(L)), 
                next : pi(O)) 
{


    h : sigma(L, bool) := head(l);

    if (h[1]) {
        b : L := body(l);
        my_while(b, head, body, next);
    } else {
        next(l);
    }
}
