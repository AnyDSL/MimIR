def count(n : int, yield : pi(int, pi()), ret : pi()) {
    yield(1, lambda() {
        def loop(i : int) {
            if (i < n)
                yield(i, lambda() { loop(i+1); });
            else
                ret();
        }
        loop(2);
    });
}

def main(n : int) -> int {
    b : int = 2;
    foreach (x : int  <- count(10)) {
        b += x;
    }

    x : int = 42;
    foreach (x  <- count(10)) {
        b += x;
    }

    return b;
}
