def count(n : float, yield : pi(float, pi()), ret : pi()) {
    yield(1.0f, lambda() {
        def loop(i : float) {
            if (i < n)
                yield(i, lambda() { loop(i+1.0f); });
            else
                ret();
        }
        loop(2.0f);
    });
}

def main(n : int) -> float {
    b : float = 2.0f;
    foreach (x : float  <- count(10.0f)) {
        b += x;
    }

    x : float = 42.0f;
    foreach (x  <- count(10.0f)) {
        b += x;
    }

    return b;
}
