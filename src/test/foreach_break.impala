/*
foreach with a break
expected result: 15
*/
def count(n : int, yield : pi(int, pi()), ret : pi()) {
    def loop(i : int) {
        if (i < n)
            yield(i, lambda() { loop(i+1); });
        else
            ret();
    }
    loop(0);
}

def main() -> int {
    b : int = 0;
    foreach (x : int  <- count(10)) {
        b += x;
        if (b > 10)
            break;
    }
    return b;
}
