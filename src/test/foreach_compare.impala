def count(n : int, yield : pi(int, pi()), ret : pi()) {
    yield(1, lambda() {
        def loop(i : int) {
            if (i < n)
                yield(i, lambda() { loop(i+1); });
            else
                ret();
        }
        loop(2);
    });
}

def main(main_ret : pi(int)) {
    b : int = 2;
    count(10, lambda(x : int, next : pi()) {
        b += x;
        next();
    },
    lambda() {
        main_ret(b);
    });
    //return b;
}
