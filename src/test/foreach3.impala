def count<T>(n : T, yield : pi(T, pi()), ret : pi()) {
    def loop(i : T) {
        if (true)
            yield(i, lambda() { loop(); });
        else
            ret();
    }
    loop(n);
}

def main(n : int) -> float {
    b : float = 2.0f;
    foreach (x : float  <- count(10.0f)) {
        b += x;
    }

    a : int = 0;
    x : int = 42;
    foreach (x  <- count(10)) {
        a += x;
    }

    return b;
}
